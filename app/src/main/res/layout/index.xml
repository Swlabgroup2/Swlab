<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.swlab.myapplication.Main_Activity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/btn1"
        android:layout_width="109dp"
        android:layout_height="104dp"
        android:text="@string/function2"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/btn5"
        android:layout_marginRight="51dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="109dp"
        android:layout_height="104dp"
        android:text="@string/function1"
        app:layout_constraintRight_toLeftOf="@+id/btn6"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/btn4"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn3"
        android:layout_width="109dp"
        android:layout_height="104dp"
        android:text="@string/function3"
        app:layout_constraintRight_toLeftOf="@+id/btn5"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="36dp" />

    <Button
        android:id="@+id/btn4"
        android:layout_width="109dp"
        android:layout_height="104dp"
        android:text="@string/function5"
        app:layout_constraintRight_toLeftOf="@+id/btn1"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/btn3"
        app:layout_constraintHorizontal_bias="0.505" />

    <Button
        android:id="@+id/btn5"
        android:layout_width="109dp"
        android:layout_height="104dp"
        android:text="@string/function4"
        android:layout_marginRight="51dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="36dp" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="109dp"
        android:layout_height="104dp"
        android:text="@string/function6"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/btn1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="51dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
